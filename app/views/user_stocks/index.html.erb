<h2> My Portfolio</h2>
<%= link_to 'Back', root_path %> 
<% if @user_stock %>
<%= @stock_details %>
<table>
	<tr>
		<th>Company</th>
		<th>Shares</th>
		<th>User_id</th>
	</tr>
	<% @user_stock.each do |user_stock| %>
	<tr>
		<td><%= user_stock.id %></td>
		<td><%= user_stock.shares %></td>
		<td><%= user_stock.user_id %></td>
	</tr>
<%end%>
</table> 
<%end%>